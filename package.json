{
  "name": "tunnelist",
  "version": "0.0.1",
  "description": "Open-source public tunneling for your localhost",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "nodemon --delay 1 index.mjs",
    "lint": "npx standard '**/*.mjs'"
  },
  "keywords": [
    "tunnel",
    "public"
  ],
  "author": "Le Phuong",
  "license": "MIT",
  "dependencies": {
    "@types/ssh2": "^0.5.36",
    "eslint": "^5.6.1",
    "keypair": "^1.0.1",
    "ssh2": "^0.6.1"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "standard": "^12.0.1"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.?(m)js?(x)",
      "**/?(*.)(spec|test).?(m)js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "mjs"
    ],
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          [
            "transform-es2015-modules-commonjs",
            {
              "spec": true
            }
          ]
        ]
      }
    }
  }
}
